{
    "name": "boekkooi/doctrine-jack-bundle",
    "type": "symfony-bundle",
    "keywords": [ "symfony", "doctrine", "extension", "inheritance mapping", "entity" ],
    "description": "Handy additional features for Doctrine 2 ORM",
    "require": {
        "php": ">=5.3.3",
        "doctrine/orm": "~2.4",
        "symfony/framework-bundle": "~2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Warnar Boekkooi",
            "email": "warnar@boekkooi.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "Boekkooi\\Bundle\\DoctrineJackBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Boekkooi\\Bundle\\DoctrineJackBundle\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
